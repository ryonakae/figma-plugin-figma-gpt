{
  "json.schemas": [
    {
      "fileMatch": ["package.json"],
      "url": "https://yuanqing.github.io/create-figma-plugin/figma-plugin.json"
    }
  ],
  "path-intellisense.mappings": {
    "@": "${workspaceFolder}/src"
  },
  "biome.enabled": false,
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
